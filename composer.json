        "cgdsoftware/migrator": "^2.0",
        "cgdsoftware/menus": "^5.0",
        "cgdsoftware/notifications": "^4.1",
        "cgdsoftware/people": "^4.0",
        "cgdsoftware/permissions": "^5.0",
        "cgdsoftware/rememberable": "^3.0",
        "cgdsoftware/roles": "^5.0",
        "cgdsoftware/searchable": "^2.2",
        "cgdsoftware/sentry": "^1.0",
        "cgdsoftware/select": "^4.0",
        "cgdsoftware/track-who": "^2.0",
        "cgdsoftware/tables": "^4.0",
        "cgdsoftware/upgrade": "^2.5",
        "cgdsoftware/users": "^2.0",
        "voku/anti-xss": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Core\\": "src/",
            "LaravelEnso\\Core\\Database\\Factories\\": "database/factories/",
            "LaravelEnso\\Core\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Core\\AppServiceProvider",
                "LaravelEnso\\Core\\BroadcastServiceProvider",
                "LaravelEnso\\Core\\EventServiceProvider",
                "LaravelEnso\\Core\\PasswordServiceProvider",
                "LaravelEnso\\Core\\MiddlewareServiceProvider",
                "LaravelEnso\\Core\\WebsocketServiceProvider"
            ]
        }
    }
