        "laravel/sanctum": "^2.3",
        "cgdsoftware/action-logger": "^3.1",
        "cgdsoftware/avatars": "^4.0",
        "cgdsoftware/charts": "^4.0",
        "cgdsoftware/companies": "^4.0",
        "cgdsoftware/data-export": "^3.0",
